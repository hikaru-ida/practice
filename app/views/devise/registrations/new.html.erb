  <fieldset>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>


    <div class="left col-sm-4 col-sm-offset-1">
      <div class="form-group">
        <div class="col-lg-10">
          <%= f.text_field :username, id: "inputUsername", class: "form-control", autofocus: true, autocomplete: "username", placeholder:"ユーザー名" %>
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-10">
        <%= f.email_field :email,id: "inputEmail", class: "form-control", autofocus: true, autocomplete: "email", placeholder:"メールアドレス" %>
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-10">
        <%= f.password_field :password,id: "inputPassword", class: "form-control", autofocus: true, autocomplete: "off", placeholder: "パスワード (#{@minimum_password_length}文字以上)" %>
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-10">
        <%= f.password_field :password,id: "inputconfirmation", class: "form-control", autofocus: true, autocomplete: "off", placeholder: "パスワードの確認" %>
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-10">
          <%= f.submit "ゴイケンに登録", class: "btn btn-primary btn-block" %>
        </div>
      </div>
    </div>
    <% end %>
    <%= render "devise/shared/links" %>

  <fieldset>